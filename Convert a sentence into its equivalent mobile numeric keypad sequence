class Solution:

    def printSequence(self,S):
        # code here
        a=["2","22","222",
            "3","33","333",
            "4","44","444",
            "5","55","555","6","66","666","7","77","777","7777",
            "8","88","888","9","99","999","9999"]
        aa=""
        for i in S:
            if i== ' ':
                aa+=str(0)
            else:
                n=ord(i)-92+1
                aa+=a[n]
        return aa
